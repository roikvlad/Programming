p = 0
t = 0
l = 0
s = 1 
j=0

st=str(input("Введите стоп слово="))
a=(hallo, klempner, das, ist, fantastisch, fluggegecheimen)
arr = len(a) - 1
print(len(a))


for i in range (0, arr, 1) :
    if a[i] == chr(' ') or a[i] == chr(',') :
        l += 1
        
m = arr - l
c = len(st)
for i in range (0, c, 1) : 
    b = 0;
    for j in range (0, j, 1) :
        if st[i] == a[j]:
            b += 1
        if j == m - 1:
            j == m - 1
            
s *= b / m
if i == c - 1 :
        print(round(s, 16))
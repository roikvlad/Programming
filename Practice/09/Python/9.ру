h1, m1 = map(int, input("Введите время первого человека(час, двоеточие, минуты)=").split(':'))
if h1 >= 0 and h1 <= 23 and m1 >= 0 and m1 <= 59 :
    if h1 >= 0 and h1 <= 23 and m1 >= 0 and m1 <= 59 :
        h2, m2 = map(int, input("Введите время второго человека(час, двоеточие, минуты)=").split(':'))
        if h1 == h2 and abs(m2 - m1) <= 15 and abs(m1 - m2) <= 15 :
            print("Встреча состоится!")
        elif (h2 - h1 == 1 and ((60 - m1) + m2) <= 15 or h2 - h1 == -1 and ((60 - m2) + m1) <= 15 or h2 - h1 == 23 and ((60 - m2) + m1) <= 15 or h2 - h1 == -23 or ((60 - m1) + m2) <= 15) :
            print("Встреча состоится")
        else:
            print("Встреча не состоится!")
    else:
        print("Неправильно введено время второго человека")
else:
    print("Неправильно введено время первого человека")
